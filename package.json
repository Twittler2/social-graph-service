    "test": "test"
  },
  "scripts": {
    "build": "babel ./server -d build/server",
    "build:watch": "babel ./server -d build/server --watch",
    "start:dev": "parallelshell 'npm run build:watch' 'nodemon ./build/server/index.js' ",
